<?
include 'function-pre.php';

$name="TimeSeriesClear">

$declaration="
object.TimeSeriesClear()
";

$parameters="
" . pt() . "object
" . pd() . "Required.  Always the name of a <tt>R2DToo.FrontEnd</tt> object.
";

$returnValue="
None.
";

$remarks="
Clears all recorded timeseries data.  Does not remove any timeseries (see " . ref("TimeSeriesRemoveAll">).
";

$example="
' [VBScript]

' load
Dim R2DToo
Set R2DToo = Wscript.CreateObject(\"R2DToo.FrontEnd\")

' set data to record
R2DToo.TimeSeriesAdd(\"Average alive\")
' run...
R2DToo.SimRun()
' ...wait for 60 seconds...
WScript.Sleep(60000)
' ...stop
R2DToo.SimPause()
' erase data collected so far
R2DToo.TimeSeriesClear()

' unload
Set R2DToo = nothing
";

$seeAlso="
" . ref("TimeSeriesAdd") . ", " . ref("TimeSeriesRemoveAll">.
";